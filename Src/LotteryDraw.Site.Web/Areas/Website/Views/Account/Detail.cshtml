@{
    ViewBag.Title = "Detail";
    Layout = "~/Areas/Website/Views/Shared/_Layout_Vip.cshtml";
}

@model LotteryDraw.Site.Models.MemberView
@section  LeftMenuItems{
    @Html.Partial("AccountNavPartial")
}

@section  BreadCrumbs{
    &gt;
    <label>详细信息</label>
}
<table class="message_table">
    <tbody>
        <tr>
            <td class="text">用户名</td>
            <td class="input">
                <label>@Model.UserName</label>
            </td>
        </tr>
        <tr>
            <td class="text">昵称</td>
            <td class="input">
                <label>@Model.Name</label>
            </td>
        </tr>

        <tr>
            <td class="text">地址</td>
            <td class="input">
                <label>@Model.Address</label>
            </td>
        </tr>
        <tr>
            <td class="text">Email</td>
            <td class="input">
                <label>@Model.Email</label>
            </td>
        </tr>
        <tr>
            <td class="text">电话</td>
            <td class="input">
                <label>@Model.Tel</label>
            </td>
        </tr>
        <tr>
            <td class="text">域名</td>
            <td class="input">
                <label>@Model.AdvertisingUrl</label>
            </td>
        </tr>
    </tbody>
</table>


